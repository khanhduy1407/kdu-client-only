{
  "name": "kdu-no-ssr",
  "version": "1.1.1",
  "description": "Kdu component to wrap non SSR friendly components",
  "repository": {
    "url": "khanhduy1407/kdu-no-ssr",
    "type": "git"
  },
  "main": "dist/kdu-no-ssr.common.js",
  "unpkg": "dist/kdu-no-ssr.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "xo",
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "bili --format cjs",
    "build:umd": "bili --format umd --compress",
    "prepublish": "npm run build",
    "example": "poi"
  },
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist"
  },
  "author": "NKDuy",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bili": "^0.16.0-rc.1",
    "eslint-config-rem": "^3.0.0",
    "poi": "^9.3.1",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem/prettier",
    "ignores": [
      "example/**"
    ]
  }
}
